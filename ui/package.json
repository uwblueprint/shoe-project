{
  "name": "shoe-project-ui",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend UI for the Shoe Project",
  "main": "index.js",
  "repository": "https://github.com/uwblueprint/shoe-project/",
  "author": "UWBlueprint and Contributors",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "parcel src/index.html",
    "watch": "parcel watch src/index.html --hmr-port 42967",
    "build": "parcel NODE_ENV=development parcel build src/index.html --no-minify",
    "build-prod": "parcel build src/index.html",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier --check src/",
    "lint:eslint": "eslint src/ --cache --format stylish",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:prettier": "prettier --write src/",
    "fix:eslint": "eslint src/ --format stylish --fix",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@material-ui/core": "4.11.1",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.11.3",
    "leaflet": "^1.0.2",
    "leaflet.markercluster": "^1.4.1",
    "material-ui-dropzone": "^3.5.0",
    "mui-virtualized-table": "3.0.0-6",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-is": "^16.13.1",
    "react-leaflet": "^2.0.0",
    "react-leaflet-control": "^2.1.2",
    "react-leaflet-markercluster": "^2.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "swr": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/leaflet": "^1.5.19",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "jest": "^26.5.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "node-forge": "0.10.0",
    "@babel/preset-env": "7.13.8"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.5"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/src/__tests__/test.init.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/jest.setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(spec|test).[jt]s?(x)"
    ]
  }
}
